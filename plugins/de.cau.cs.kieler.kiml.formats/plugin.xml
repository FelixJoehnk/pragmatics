<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="graphTransformers" name="Graph Transformers" schema="schema/graphTransformers.exsd"/>

   <extension
         point="de.cau.cs.kieler.kiml.formats.graphTransformers">
      <handler
            class="de.cau.cs.kieler.kiml.formats.KGraphHandler"
            description="Internally used graph format of the KIELER project. See http://rtsys.informatik.uni-kiel.de/confluence/x/U4EN"
            extensions="kgraph,kgx,xmi"
            id="de.cau.cs.kieler.kgraph"
            name="KGraph">
      </handler>
      <handler
            class="de.cau.cs.kieler.kiml.formats.graphml.GraphMLHandler"
            description="The Graph Markup Language. See http://graphml.graphdrawing.org/"
            extensions="graphml"
            id="org.graphdrawing.graphml"
            name="GraphML">
      </handler>
      <handler
            class="de.cau.cs.kieler.kiml.formats.matrix.MatrixHandler"
            description="The Matrix Market format. See http://math.nist.gov/MatrixMarket/"
            extensions="mtx,matrix"
            id="gov.nist.math.matrix"
            name="Matrix">
      </handler>
      <handler
            class="de.cau.cs.kieler.kiml.formats.ogml.OgmlHandler"
            description="The Open Graph Markup Language."
            extensions="ogml"
            id="net.ogdf.ogml"
            name="OGML">
      </handler>
   </extension>

</plugin>
